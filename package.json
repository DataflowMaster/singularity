  "bugs": {
    "url": "https://github.com/DataflowMaster/singularity/issues"
  },
  "homepage": "https://github.com/DataflowMaster/singularity#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "mocha": "^7.0.1"
  },
  "dependencies": {
    "testdouble": "^3.12.5"
  }
}
