{
  "name": "singularity",
  "version": "1.0.0",
  "description": "Code with domain driven development structure on the response form of a person.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "babel": "./node_modules/.bin/babel src --out-dir lib --presets=@babel/env",
    "clean": "rm -rf lib ",
    "src": "babel src --out-dir lib/src/ --presets=@babel/env",
    "spec": "babel test --out-dir lib/test/ --presets=@babel/env",
    "test": "npm run clean && npm run src && npm run spec && mocha lib/test/*/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataflowMaster/singularity.git"
  },
